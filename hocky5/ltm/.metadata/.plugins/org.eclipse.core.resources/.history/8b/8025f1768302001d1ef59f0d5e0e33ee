package pop3_tcp;

public class Student {
	private String id;
	private String name;
	private int age;
	private float score;

	public Student(String id, String name, int age, float score) {
		super();
		this.id = id;
		this.name = name;
		this.age = age;
		this.score = score;
	}

	public Student() {
		super();
	}

}
